{
  "name": "decensored-deepseekr1-for-vscode",
  "publisher": "rohan-patnaik",
  "displayName": "Decensored DeepseekR1 for VScode",
  "description": "A decensored Deepseek R1 extension for VS Code open sourced by perplexity.ai to be used with vscode as an extension.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:decensored-deepseekr1-for-vscode.chico"
  ],
  "main": "./out/extension.js",
  "icon": "images/extension-logo.png",
  "contributes": {
    "commands": [
      {
        "command": "decensored-deepseekr1-for-vscode.chico",
        "title": "Open Deepseek Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "ollama": "^0.5.13"
  },
  
  "repository": {
    "type": "git",
    "url": "https://github.com/rohan-patnaik/decensored-deepseekR1-for-vscode"
  }
  
}
